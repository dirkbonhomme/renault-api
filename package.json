{
  "name": "@remscodes/renault-api",
  "version": "1.0.0",
  "description": "Resources url and models to use the Renault API",
  "license": "MIT",
  "author": {
    "name": "RÃ©my Abitbol",
    "url": "https://github.com/remscodes"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "types": "tsc --noEmit",
    "rm-dist": "node ./scripts/rm-dist.cjs",
    "prebuild": "npm run rm-dist"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "node": "./dist/cjs/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remscodes/renault-api"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.16",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "renault"
  ],
  "packageManager": "pnpm@8.7.5",
  "sideEffects": false
}
